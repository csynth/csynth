{
  "name": "organicart",
  "main": "threek.html",
  "description": "organic art using WebGL",
  "version": "0.1.0",
  "keywords": [
    "organic",
    "art",
    "node-webkit"
  ],
  "window": {
    "toolbar": false,
    "fullscreen": false,
    "deferfullscreen": false,
    "frame": false,
    "NOwidth": 1920,
    "wnote": "set width and height if we really want to force values",
    "NOheight": 1080,
    "min_width": 400,
    "min_height": 200,
    "NOleft": 0,
    "NOtop": 0,
    "note": "position does not work well with multiple screens, so we use our own left/top"
  },
  "_comment": "see https://github.com/rogerwang/node-webkit/wiki/Manifest-format",
  "private": true,
  "devDependencies": {
    "@types/js-yaml": "^3.12.7",
    "@types/node": "^16.14.2",
    "@types/three": "^0.149.0",
    "@types/ws": "^6.0.4",
    "chokidar": "^3.5.2",
    "esbuild": "^0.15.14",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "esprima": "^4.0.1",
    "patch-package": "^6.5.0",
    "rollup-plugin-copy": "^3.4.0",
    "ts-loader": "^8.4.0",
    "tslib": "^2.5.0",
    "tslint": "^6.1.3",
    "vite": "^4.1.4",
    "vite-plugin-singlefile": "^0.13.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@supercollider/osc": "^1.0.0",
    "@tonaljs/modules": "^3.4.16",
    "clipboardy": "^3.0.0",
    "js-yaml": "^3.14.1",
    "lil-gui": "^0.17.0",
    "openvr.js": "^0.2.9",
    "qrcode": "^1.5.1",
    "supercolliderjs": "^1.0.1",
    "terser": "^5.22.0",
    "three": "^0.161.0",
    "typescript": "^4.9.5",
    "vite-plugin-glsl": "^1.1.2",
    "webrtc.io": "^0.0.4",
    "ws": "^7.5.9"
  },
  "scripts": {
    "prepare": "patch-package",
    "server": "node dist/organserver.js",
    "server-dev": "npm run esbuild-devserver && node dist/esbuild_organserver.js",
    "esbuild-devserver": "esbuild TSServer/organserver.ts --bundle --platform=node --outfile=dist/esbuild_organserver.js --sourcemap --external:esbuild --external:typescript --external:fsevents --external:openvr.js --minify",
    "esbuild-server": "esbuild TSServer/organserver.ts --bundle --platform=node --outfile=dist/organserver.js --sourcemap --minify",
    "esbuild-tadmus": "esbuild TS/mod/sketch/tadmus.ts --bundle --external:esbuild --outfile=dist/tadmus.esbuild.js --sourcemap --minify",
    "webpack": "webpack",
    "webpackw": "webpack -w",
    "tscw": "npx tsc -p TS --watch",
    "tsc": "npx tsc -p TS",
    "tscversion": "npx tsc --version",
    "viteControls_dev": "vite --host",
    "viteControls_build": "vite build",
    "_vite_dev_OldExperiment": "node vite_scripts/vite_dev.js",
    "_vite_build_OldExperiment": "node vite_scripts/vite_build.js"
  },
  "_//browser": "for webpack web output",
  "browser": {
    "fs": false,
    "http": false,
    "child_process": false,
    "electron": false,
    "os": false,
    "path": false,
    "fluent-ffmpeg": false,
    "net": false,
    "nw.gui": false,
    "dgram": false,
    "https": false
  }
}
